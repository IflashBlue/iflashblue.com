<div class="container mx-auto px-5 py-2 lg:px-32">
    <div class="-m-1 flex flex-wrap md:-m-2">
        {% for image in images %}
            <div class="flex w-full md:w-1/4 flex-wrap">
                <div class="w-full p-1 md:p-2">
                    <img alt="gallery" class="block h-full w-full rounded-lg object-cover object-center" src="{{ image.image }}" />
                </div>
            </div>
        {% endfor %}
    </div>
</div>